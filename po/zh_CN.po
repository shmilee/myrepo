msgstr "另外%s个软件不在list_AUR中：\\n%s"

msgid ""
"Use 'add [names]' or 'del [names]' to add(delete) package to(from) list_AUR."
msgstr "用命令'add [names]'或'del [names]'在list_AUR中添加或删除软件名。"

msgid "Use 'finish' to exit this loop."
msgstr "用命令'finish'结束添加或删除。"

msgid "Enter you command: "
msgstr "请输入您的命令："

