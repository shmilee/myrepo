msgstr "%s个软件升级完成。"

msgid "Success: %s\\n%s"
msgstr "成功：%s个\\n%s"

msgid "Fail: %s\\n%s"
msgstr "失败：%s个\\n%s"

msgid "%s packages not found in AUR:\\n%s"
msgstr "%s个软件没在AUR中找到：\\n%s"

msgid ""
"These packages may be moved into repo community, or your network cannot "
"connect to AUR %s."
msgstr "这些软件可能已被移到community仓库，或者是因为你的网络无法连接到AUR %s."

msgid "Overview, no git or svn package in repo: %s."
msgstr "概述，软件源%s中无 -git -svn 类的软件。"

msgid "Overview, %s git or svn packages need to update:\\n%s"
msgstr "概述，%s个 -git -svn 类的软件需要升级：\\n%s"

msgid "(%s/%s) %s :"
msgstr "(%s/%s) %s : "

msgid "Pass: %s\\n%s"
msgstr "跳过：%s个\\n%s"

